# AgentCoin API Configuration
# Copy this file to .env and fill in your actual values

# AgentCoin API endpoint
AGENTCOIN_API_URL=http://localhost:3000/api

# Your AgentCoin API key (get from AgentCoin dashboard)
AGENTCOIN_API_KEY=your_api_key_here

# Your agent's wallet address
AGENTCOIN_AGENT_ADDRESS=your_agent_address_here

# Network to use (testnet for development, mainnet for production)
AGENTCOIN_NETWORK=testnet

# Optional: Enable debug logging
# DEBUG=agentcoin:*